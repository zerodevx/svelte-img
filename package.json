{
  "name": "@zerodevx/svelte-img",
  "version": "1.2.4",
  "description": "High-performance responsive images for SvelteKit",
  "author": "Jason Lee <jason@zerodevx.com>",
  "scripts": {
    "dev": "NODE_ENV=development vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "git checkout gh-pages && rm -rf _app && cp -R build/ . && git add -A && git commit -m 'Update' && git checkout master && git push origin gh-pages",
    "package": "svelte-kit sync && svelte-package",
    "publish": "cd package && npm publish --access public",
    "publish:next": "cd package && npm publish --access public --tag next",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "dependencies": {
    "imagetools-core": "3.3.1",
    "vite-imagetools": "4.0.17"
  },
  "devDependencies": {
    "@playwright/test": "1.30.0",
    "@sveltejs/adapter-static": "1.0.5",
    "@sveltejs/kit": "1.3.1",
    "@sveltejs/package": "1.0.2",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.13",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-svelte3": "4.0.0",
    "is-port-reachable": "4.0.0",
    "postcss": "8.4.21",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.3",
    "prettier-plugin-svelte": "2.9.0",
    "svelte": "3.55.1",
    "svelte-check": "3.0.3",
    "svelte-preprocess": "5.0.1",
    "tailwindcss": "3.2.4",
    "tslib": "2.5.0",
    "typescript": "4.9.4",
    "vite": "4.0.4"
  },
  "license": "ISC",
  "repository": "github:zerodevx/svelte-img",
  "keywords": [
    "svelte",
    "sveltekit",
    "responsive",
    "progressive",
    "image",
    "lazy-load"
  ],
  "type": "module"
}
