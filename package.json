{
  "name": "@zerodevx/svelte-img",
  "version": "2.1.1",
  "description": "High-performance responsive/progressive images for SvelteKit",
  "author": "Jason Lee <jason@zerodevx.com>",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run lint && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint && npm run check",
    "test": "playwright test",
    "package": "npm run lint && svelte-package && node generate-tsd && npx publint",
    "deploy": "npx gh-pages -d build -t -f"
  },
  "dependencies": {
    "vite-imagetools": "^6.2.9"
  },
  "peerDependencies": {
    "svelte": "^3.55.1 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@fontsource-variable/inter": "^5.0.20",
    "@iconify-json/mdi": "^1.1.68",
    "@iconify/tailwind": "^1.1.3",
    "@playwright/test": "^1.46.1",
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/typography": "^0.5.14",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sveld": "^0.20.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "default": "./dist/vite.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "license": "ISC",
  "repository": "github:zerodevx/svelte-img",
  "keywords": [
    "svelte",
    "sveltekit",
    "responsive",
    "progressive",
    "image",
    "lazy-load"
  ]
}
